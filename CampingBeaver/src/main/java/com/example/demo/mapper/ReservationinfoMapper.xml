		insert into reservation_info values(null, now(), #{user_id}, #{pkg_seq}, #{reserv_name}, #{reserv_addr}, #{reserv_post}, #{reserv_phone}, #{reserv_price}, #{reserv_pay}, #{reserv_s_date}, #{reserv_e_date})
